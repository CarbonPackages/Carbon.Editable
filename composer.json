    "license": "MIT",
    "keywords": [
        "flow",
        "neos",
        "fusion",
        "helper",
        "carbon"
    ],
    "require": {
        "neos/neos": "~3.3 || ~4.0"
    },
    "authors": [
        {
            "name": "Jon Uhlmann",
            "email": "me@jonnitto.ch",
            "homepage": "https://jonnitto.ch",
            "role": "Developer"
        }
    ],
    "archive": {
        "exclude": [
            ".editorconfig",
            ".gitattributes",
            ".github",
            ".gitignore",
            "CODE_OF_CONDUCT.md"
        ]
    },
    "autoload": {
        "psr-4": {
            "Carbon\\Editable\\": "Classes"
        }
    },
    "extra": {
        "marketplace-account": "me@jonnitto.ch",
        "installer-name": "Carbon.Editable",
        "neos": {
            "package-key": "Carbon.Editable"
        }
    }
